/*------------------------------------*\
  Home styles
\*------------------------------------*/

//= depend_on_asset "m1/intel.jpg"

.welcome-block {
  background-image: url(<%= asset_path 'm1/intel.jpg' %>);
  background-size: cover;
  width: 100%;
  height: 600px;
}

.welcome-block__title {
  position: relative;
  top: 100px;
  font-size: em(40);
  font-weight: 600;
  color: $cm-agrey;
  span {
    display: block;
    font-style: italic;
  }
}